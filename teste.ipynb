   "source": [
    "import redis\n",
    "import time\n",
    "rd = redis.Redis(\"127.0.0.1\")\n",
    "inicio = time.time()\n",
    "rd.set(\"inicio\", inicio)\n",
    "time.sleep(2)\n",
    "del inicio\n",
    "inicio = float(rd.get(\"inicio\").decode(\"utf-8\"))\n",
    "print(inicio)\n",
    "final = time.time() - inicio\n",
    "print (f\"tempo final {final:.2f} segundos\")\n"
   ]
  }
